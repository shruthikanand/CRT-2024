import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner s =new Scanner(System.in);
       int n = s.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++)
        {
            a[i]=s.nextInt();
        }
        int c=0;
        for(int i=0;i<n;i++)
        {
             if (Math.sqrt(a[i]) == (int)Math.sqrt(a[i]))
                 c++;
        }
        System.out.println(c);
    }
}
