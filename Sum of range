import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int min,max;
        min=s.nextInt();
        max=s.nextInt();
        int i,sum=0;
        for(i=min;i<=max;i++)
        {
             sum=sum+i;
        }
        System.out.println(sum);
    }
}
