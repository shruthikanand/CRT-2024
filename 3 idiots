import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int x1,y1,x2,y2;
        x1=s.nextInt();
        y1=s.nextInt();
        x2=s.nextInt();
        y2=s.nextInt();
        float a,b;
        a=(float)(x1+x2)/2;
        b=(float)(y1+y2)/2;
        System.out.printf("Binoy's house is located at (%.1f,%.1f)",a,b);
    }
}
