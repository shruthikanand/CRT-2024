import java.util.*;
class Building
{
 public static void main(String args[])
{
 Scanner s=new Scanner(System.in);
 int n;
n=s.nextInt();
int a[ ]= new int[5];
for(int i=0;i<n;i++)
{
  a[i]=s.nextInt();
}
int c=0;
for(int i=0;i<n;i++)
{
  for(int j=i+1;j<n-1;j++)
  {
    if(a[i]>a[j])
    {
       c=c+1;
     }
  }
}
System.out.println(c);
}
}
