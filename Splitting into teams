import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int a,b,c,d;
        a=s.nextInt();
        b=s.nextInt();
        c=a/b;
        d=a%b;
        System.out.printf("The number of students in each team is %d and left out is %d",c,d);
    }
}
