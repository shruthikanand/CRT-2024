import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int a,b,c;
        int r1, r2, rem, r3;
        a = s.nextInt();
        b = s.nextInt();
        c = s.nextInt();
        r1 = (a*b)/100;
        System.out.println(r1);
        rem = a-r1;
        r2 = (c*rem)/100;
        System.out.println(r2);
        r3 = (a-(r1+r2))/3;
        System.out.println(r3);
    }
}
