import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int a,b,c;
        a=s.nextInt();
        b=s.nextInt();
        c=a*b;
        if(c>1000)
        {
            System.out.println(a+b);
        }
        else
        {
            System.out.println(c);
        }
    }
}
